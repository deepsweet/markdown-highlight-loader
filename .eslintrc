{
  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": 1,
    "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 1,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,

    "block-scoped-var": 0,
    "complexity": 0,
    "consistent-return": 1,
    "curly": 2,
    "default-case": 1,
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 1,
    "no-else-return": 2,
    "no-empty-label": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 1,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-void": 2,
    "no-warning-comments": [1, { "terms": ["todo"], "location": "start" }],
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "yoda": 2,

    "global-strict": [0, "always"],

    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-undef": 0,
    "no-undef-init": 1,
    "no-undefined": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 2,

    "handle-callback-err": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-restricted-modules": 0,
    "no-sync": 0,

    "brace-style": [1, "1tbs", { "allowSingleLine": false }],
    "camelcase": 1,
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "consistent-this": [2, "self"],
    "eol-last": 2,
    "func-names": 0,
    "func-style": 0,
    "key-spacing": 0,
    "max-nested-callbacks": 0,
    "new-cap": 0,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-inline-comments": 0,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-nested-ternary": 2,
    "no-new-object": 1,
    "semi-spacing": [2, {"before": false, "after": true}],
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 0,
    "no-wrap-func": 2,
    "one-var": 0,
    "operator-assignment": 2,
    "padded-blocks": 0,
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "sort-vars": 0,
    "space-before-function-parentheses": [2, "never"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [1, "always"],
    "space-in-brackets": [1, "always", {
      "singleValue": true,
      "objectsInArrays": true,
      "arraysInArrays": true,
      "arraysInObjects": true,
      "objectsInObjects": false,
      "propertyName": false
    }],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "spaced-line-comment": [2, "always"],
    "wrap-regex": 0,

    "max-depth": 0,
    "max-len": [1, 90, 4],
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 1,
    "no-plusplus": 1
  }
}
